<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <div class="form">
     <%= form_for @vendor , :html => { :multipart => true } do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name %>
     <br><br>
      <%= f.label :address %>
      <%= f.text_field :address %>
      <br><br>
      <%= f.label :telno %>
      <%= f.text_field :telno %>
      <br><br>
       <%= f.label :image %>
       <%= f.file_field :image %>

     <br><br>
      <%= f.submit "Create my account", class: "btn btn-primary" %>
    <% end %>
 
    </div>
      </div>
</div>